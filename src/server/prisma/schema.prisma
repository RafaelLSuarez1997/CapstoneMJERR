generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userId   Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Items {
  id          Int    @id @default(autoincrement())
  brand       String
  category    String
  imageUrl    String
  size        Float
  price       Float
  description String
}
